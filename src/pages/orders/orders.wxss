.menu-item {
  width: 100%;
  background-color: #3399FF;
  color: #fff;
  margin-bottom: 12rpx;
  display: flex;
  align-items: center;
}

.menu-item-content {
  width: 100%;
  padding: 12rpx;
}

.menu-item-line {
  width: 100%;
  height: 42rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 12rpx 0;
}

.menu-item-line-left {
}

.menu-item-line-right {
}

.menu-item text {
  padding: 0 14rpx;
}

.menu-item-img-left {
  width: 46rpx;
  height: 46rpx;
  display: inline-block;
  margin: 0 24rpx;
}

.menu-item-img-right {
  width: 46rpx;
  height: 46rpx;
  display: inline-block;
  margin: 0 24rpx;
}

.menu-item-text {
  display: inline-block;
  font-size: 11pt;
}

.menu-item-text-small {
  display: inline-block;
  font-size: 10pt;
}
